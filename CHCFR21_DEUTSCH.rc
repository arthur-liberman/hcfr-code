    POPUP "Anwendung"
    BEGIN
        MENUITEM "Neues Dokument",              ID_FILE_NEW
        MENUITEM "Dokument öffnen...",          ID_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "Nur existierende Ansichten anzeigen", IDM_SHOWOPENVIEWSONLY
    END
    POPUP "Dokument"
    BEGIN
        MENUITEM "Neu",                         ID_FILE_NEW
        MENUITEM "Öffnen...",                   ID_FILE_OPEN
        MENUITEM "Duplizieren",                 IDM_DUPLICATEDOC
        MENUITEM "Schließen",                   ID_FILE_CLOSE
        MENUITEM "Speichern",                   ID_FILE_SAVE
        MENUITEM "Speichern unter...",          ID_FILE_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "Als Referenzmessung setzen",  IDM_SETASREF
        MENUITEM SEPARATOR
        POPUP "Ansicht anzeigen"
        BEGIN
            MENUITEM "Histogramm &Helligkeit",      IDM_VIEW_LUMINANCEHISTO
            MENUITEM "Histogramm nahe &Schwarz",    IDM_VIEW_NEARBLACKHISTO
            MENUITEM "Histogramm nahe &Weiß",       IDM_VIEW_NEARWHITEHISTO
            MENUITEM SEPARATOR
            MENUITEM "Histogramm &RGB Niveau",      IDM_VIEW_RGBHISTO
            MENUITEM "Histogramm Farb&temperatur",  IDM_VIEW_COLORTEMPHISTO
            MENUITEM "&CIE Diagramm",               IDM_VIEW_CIECHART
            MENUITEM SEPARATOR
            MENUITEM "Histogramm Sä&ttigung - Helligkeit", IDM_VIEW_SATLUMHISTO
            MENUITEM "Histogramm Sättigungs&verschiebung", IDM_VIEW_SATLUMSHIFT
            MENUITEM SEPARATOR
            MENUITEM "Kombiniertes H&istogramm für freie Messungen", IDM_VIEW_MEASURESCOMBO
        END
        MENUITEM "Alle Ansichten anzeigen",     IDM_SHOWALLVIEWS
        MENUITEM "Alle Ansichten schließen",    IDM_CLOSEALLVIEWS
    END
    POPUP "Ansicht"
    BEGIN
        MENUITEM "Anzeigen",                    IDM_SHOWVIEW
        MENUITEM "Für alle Dokumente anzeigen", IDM_SHOWVIEWFORALLDOC
        MENUITEM "Schließen",                   IDM_CLOSEVIEW
        MENUITEM "Andere Ansichten schließen",  IDM_CLOSEOTHERVIEWS
    END
END

IDR_TAB_MENU MENU
BEGIN
    POPUP "Reiter"
    BEGIN
        MENUITEM "Reiter aktivieren",           IDM_TAB_ACTIVATE
        MENUITEM "Reiter bei allen Dokumenten aktivieren\tCtrl+Click", IDM_TAB_ACTIVATE_ON_ALL
        MENUITEM "Reiter duplizieren",          IDM_TAB_DUPLICATE
        MENUITEM "Reiter in neuem Fenster öffnen", IDM_TAB_NEWFRAME
        MENUITEM SEPARATOR
        MENUITEM "Reiter schließen",            IDM_TAB_CLOSE
    END
END

IDR_GAMMA_GRAPH_MENU MENU
BEGIN
    POPUP "Diagramm"
    BEGIN
        MENUITEM "Referenz anzeigen",           IDM_LUM_GRAPH_SHOWREF
        MENUITEM "Referenzmessung anzeigen",    IDM_LUM_GRAPH_DATAREF
        MENUITEM "Durchschnitt anzeigen",       IDM_LUM_GRAPH_SHOW_AVG
        MENUITEM SEPARATOR
        MENUITEM "Helligkeit",                  IDM_LUM_GRAPH_YLUM
        MENUITEM "Rot",                         IDM_LUM_GRAPH_REDLUM
        MENUITEM "Grün",                        IDM_LUM_GRAPH_GREENLUM
        MENUITEM "Blau",                        IDM_LUM_GRAPH_BLUELUM
        MENUITEM SEPARATOR
        MENUITEM "Speichern...",                IDM_GRAPH_SAVE
        POPUP "Skalierung"
        BEGIN
            MENUITEM "Automatisch",                 IDM_GRAPH_SCALE_FIT
            POPUP "Gamma"
            BEGIN
                MENUITEM "1-3",                         IDM_GAMMA_GRAPH_Y_SCALE1
                MENUITEM "Automatisch",                 32886
                MENUITEM "Zoom +",                      32888
                MENUITEM "Zoom -",                      32887
                MENUITEM "Nach oben",                   32885
                MENUITEM "Nach unten",                  32884
            END
            MENUITEM "Werte...     ",               IDM_GRAPH_SCALE_CUSTOM
        END
        MENUITEM "Parameter...",                IDM_GRAPH_SETTINGS
        MENUITEM SEPARATOR
        MENUITEM "&Hilfe",                      IDM_HELP
    END
END


